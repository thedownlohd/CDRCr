ggplot(data=data, aes_string(x="Abbreviation", y = column1)) + geom_point(alpha=1/4) + ggtitle(column1) + theme(axis.text.x = element_text(angle = 90, hjust = 1)) + coord_flip()
}
graphit(graph_table, graph_table$Percentage)
graphit(graph_table, Percentage)
graphit(graph_table, "Percentage")
